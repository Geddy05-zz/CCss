@model List<Simple_solutions.Models.SearchPropertiesModel>

@{
    ViewBag.Title = "Search";
}

<div id="wrapper">
    <nav>

        <ul>
            <li><a href="#">Prijsverloop per producttype</a></li>
            <li><a href="#">Meest gekochte producten</a></li>
            <li><a href="#">Meest gezochte paketten</a></li>
            <li><a href="#">Meest gezocht producten</a></li>
        </ul>

    </nav>
    <table id="searchresults">
        <tr>
            <th>
                processor Type
            </th>
            <th>
                processor ClockSpeed
            </th>
            <th>
                processor Cores
            </th>
            <th>
                memory Slots
            </th>
            <th>
                memory Speed
            </th>
            <th>
                graphic CardSpeed
            </th>
            <th>
                graphic CardType
            </th>
            <th>
                hardDrive Type
            </th>
            <th>
                hardDrive Capacity
            </th>
            <th>
                opticalDrive Category
            </th>
            <th>
                systemUnit Formfactor
            </th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.processorType)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.processorClockSpeed)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.processorcores)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.memorySlots)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.memorySpeed)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.graphicCardSpeed)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.graphicCardType)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.hardDrivetype)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.hardDriveCapacity)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.opticalDriveCategory)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.systemUnitFormfactor)
                </td>
            </tr>
        }
    </table>
</div>
